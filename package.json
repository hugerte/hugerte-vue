    "@storybook/vue3": "^6.5.13",
    "@tinymce/eslint-plugin": "^2.3.1",
    "@types/node": "^22.9.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/compiler-sfc": "^3.4.27",
    "babel-loader": "^8.2.3",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.26.0",
